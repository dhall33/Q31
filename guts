public class memo {
  private int year = 1999;
  private int month = 08;
  private String message = "This is the coolest day!";
    public memo (int year, int month, String message) {
    this.year = year;
    this.month = month;
    this.message = message;
    }
    public void setYear (int year) {
    this.year = year;
    }
    public int getYear () {
    return year;
    }
    public void setMonth (int month) {
    this.month = month;
    }
    public int getMonth () {
    return month;
    }
    public void setMessage (String message) {
    this.message = message;
    }
    public String getMessage () {
    return message;
    }
    public String toString () {
    return year + " " + month + " " + message;
    }
    }
